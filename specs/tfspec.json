    "resources": [
        {
            "name": "configlet",
            "schema": {
                "attributes": [
                    {
                        "name": "api_version",
                        "string": {
                            "computed_optional_required": "computed_optional",
                            "default": {
                                "static": "config.eda.nokia.com/v1alpha1"
                            },
                            "validators": [
                                {
                                    "custom": {
                                        "imports": [
                                            {
                                                "path": "regexp"
                                            },
                                            {
                                                "path": "github.com/hashicorp/terraform-plugin-framework-validators/stringvalidator"
                                            }
                                        ],
                                        "schema_definition": "stringvalidator.RegexMatches(regexp.MustCompile(\"^config\\\\.eda\\\\.nokia\\\\.com/v1alpha1$\"), \"\")"
                                    }
                                }
                            ]
                        }
                    },
                    {
                        "name": "kind",
                        "string": {
                            "computed_optional_required": "computed_optional",
                            "default": {
                                "static": "Configlet"
                            },
                            "validators": [
                                {
                                    "custom": {
                                        "imports": [
                                            {
                                                "path": "regexp"
                                            },
                                            {
                                                "path": "github.com/hashicorp/terraform-plugin-framework-validators/stringvalidator"
                                            }
                                        ],
                                        "schema_definition": "stringvalidator.RegexMatches(regexp.MustCompile(\"^Configlet$\"), \"\")"
                                    }
                                }
                            ]
